{
	"name": "@depthbomb/fs-utils",
	"description": "Common FileSystem utilities",
	"version": "0.1.0",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/depthbomb/common.git",
		"directory": "packages/fs-utils"
	},
	"bugs": {
		"url": "https://github.com/depthbomb/common/issues"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup"
	},
	"dependencies": {
		"convert": "^4.5.0",
		"pathe": "^0.3.1",
		"uid": "^2.0.0"
	},
	"packageManager": "yarn@3.2.1",
	"tsup": {
		"clean": true,
		"entry": [
			"src/index.ts"
		],
		"format": [
			"cjs",
			"esm"
		],
		"noExternal": [
			"uid",
			"pathe",
			"convert"
		],
		"minify": true,
		"skipNodeModulesBundle": false,
		"target": "node16",
		"tsconfig": "./tsconfig.json",
		"dts": true,
		"splitting": true,
		"sourcemap": false,
		"keepNames": false
	}
}
