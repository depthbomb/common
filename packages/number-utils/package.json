{
	"name": "@depthbomb/number-utils",
	"description": "Common number utilities",
	"version": "0.1.0",
	"scripts": {
		"build": "tsup"
	},
	"packageManager": "yarn@3.2.1",
	"tsup": {
		"clean": true,
		"entry": [
			"src/index.ts"
		],
		"format": [
			"cjs",
			"esm"
		],
		"minify": true,
		"skipNodeModulesBundle": false,
		"target": "node16",
		"tsconfig": "./tsconfig.json",
		"dts": true,
		"splitting": true,
		"sourcemap": false,
		"keepNames": false
	}
}
